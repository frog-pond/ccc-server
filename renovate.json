{
  "extends": [
    "config:base",
    ":semanticCommitsDisabled"
  ],
  "lockFileMaintenance": {
    "enabled": true,
    "schedule": ["after 5pm on every weekday"]
  },
  "timezone": "America/Chicago",
  "labels": [
    "renovate"
  ],
  "prConcurrentLimit": 0,
  "prHourlyLimit": 0,
  "packageRules": [
    {
      "packagePatterns": ["^moment"],
      "groupName": "moment packages"
    }
  ]
}
